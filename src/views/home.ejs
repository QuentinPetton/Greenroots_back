<%- include('./partials/head.ejs') %>
  <%- include('./partials/header.ejs') %>

    <main>
      <h1>test</h1>

      <table>
        <thead>
          <tr>
            <th>Prénom</th>
            <th>Nom</th>
            <th>Email</th>
            <th>Password</th>
            <th>Téléphone</th>
            <th>Numéro de rue</th>
            <th>Rue</th>
            <th>Ville</th>
            <th>Code postal</th>
            <th>Pays</th>
            <th>Type d'entité</th>
            <th>Nom de l'entité</th>
            <th>SIRET</th>
            <th>Administrateur</th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach(user=> { %>
            <tr>
              <td>
                <%= user.firstname %>
              </td>
              <td>
                <%= user.lastname %>
              </td>
              <td>
                <%= user.email %>
              </td>
              <td>
                <%= user.password %>
              </td>
              <td>
                <%= user.phone_number %>
              </td>
              <td>
                <%= user.street_number %>
              </td>
              <td>
                <%= user.street %>
              </td>
              <td>
                <%= user.city %>
              </td>
              <td>
                <%= user.postal_code %>
              </td>
              <td>
                <%= user.country %>
              </td>
              <td>
                <%= user.entity_type %>
              </td>
              <td>
                <%= user.entity_name %>
              </td>
              <td>
                <%= user.entity_siret %>
              </td>
              <td>
                <%= user.is_admin %>
              </td>
            </tr>
            <% }) %>
        </tbody>

      </table>

    </main>


    <%- include('./partials/footer.ejs') %>